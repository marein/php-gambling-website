{
  "name": "marein/php-gaming-website",
  "description": "A gaming website where people can play against each other.",
  "license": "MIT",
  "authors": [
    {
      "name": "Markus Reinhold",
      "email": "markusreinhold@icloud.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Gaming\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gaming\\Tests\\Unit\\": "tests/unit/"
    }
  },
  "require": {
    "php": "^8.0",
    "doctrine/dbal": "^2.10",
    "doctrine/doctrine-bundle": "^2.2",
    "doctrine/doctrine-migrations-bundle": "^2.2",
    "doctrine/orm": "^2.8",
    "enqueue/amqp-lib": "^0.10.8",
    "marein/php-nchan-client": "^3.0",
    "marein/symfony-standard-headers-csrf-bundle": "^1.0",
    "predis/predis": "^1.1",
    "psr/log": "^1.1",
    "ramsey/uuid": "^3.8",
    "ramsey/uuid-doctrine": "^1.5",
    "symfony/asset": "^5.1",
    "symfony/console": "^5.1",
    "symfony/dotenv": "^5.1",
    "symfony/finder": "^5.1",
    "symfony/framework-bundle": "^5.1",
    "symfony/monolog-bundle": "^3.4",
    "symfony/twig-bundle": "^5.1",
    "symfony/validator": "^5.1",
    "symfony/yaml": "^5.1",
    "twig/twig": "^2.14"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2",
    "squizlabs/php_codesniffer": "^3.4",
    "phpstan/phpstan": "^0.12.28",
    "symfony/debug-bundle": "^5.1",
    "symfony/web-profiler-bundle": "^5.1",
    "symfony/stopwatch": "^5.1",
    "roave/security-advisories": "dev-master",
    "symfony/error-handler": "^5.1",
    "codeception/module-asserts": "^1.2",
    "codeception/module-webdriver": "^1.1",
    "codeception/codeception": "^4.1"
  },
  "config": {
    "sort-packages": true
  }
}
